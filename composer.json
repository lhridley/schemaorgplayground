{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": {
        "drupal/ats": {
            "type": "package",
            "package": {
                "name": "drupal/ats",
                "version": "1.0.x-dev",
                "type": "drupal-module",
                "source": {
                    "url": "https://git.drupalcode.org/project/ats.git",
                    "type": "git",
                    "reference": "1.0.x"
                }
            }
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "libraries": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "bower-asset/masonry": "^4.2",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/ats": "1.0.x-dev",
        "drupal/core-composer-scaffold": "9.4.x-dev",
        "drupal/core-recommended": "9.4.x-dev",
        "drupal/country": "^1.0",
        "drupal/entity": "^1.4",
        "drupal/paragraphs": "^1.15",
        "drupal/redhen": "^1.0@RC",
        "drupal/schemadotorg": "1.0.x-dev",
        "drush/drush": "^11.1",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "require-dev": {
        "drupal/core-dev": "9.4.x-dev",
        "drupal/devel": "^5.0",
        "mglaman/phpstan-drupal": "^1.1",
        "phpstan/extension-installer": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "oomphinc/composer-installers-extender": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
